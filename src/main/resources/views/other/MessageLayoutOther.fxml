<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.judgeman.WebSocketChatClient.ViewControllers.Other.MessageLayoutViewController">
   <children>
      <VBox styleClass="messageBoxBackgroundOther" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0">
         <children>
            <Label fx:id="nameTextLabel" maxWidth="300.0" text="Label" textFill="#656565">
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
            <Label fx:id="messageTextLabel" maxHeight="1.7976931348623157E308" maxWidth="300.0" text="Label" textFill="WHITE" wrapText="true">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <FlowPane alignment="CENTER_RIGHT" prefWrapLength="0.0">
               <children>
                  <Label fx:id="messageDateLabel" text="Label" />
               </children>
            </FlowPane>
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
